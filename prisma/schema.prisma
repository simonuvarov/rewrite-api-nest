generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Paper {
  id        String   @id @default(uuid())
  question  String
  body      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  overallBand      Int?
  taBand    Int?
  ccBand    Int?
  lrBand    Int?
  grBand    Int?
}

